{
  "name": "gym-app-fase1-usuarios",
  "version": "1.0.0",
  "description": "Fase 1: Sistema de gestión de usuarios con autenticación JWT y bcrypt",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "node backend/server.js",
    "dev:watch": "nodemon --signal SIGINT backend/server.js",
    "seed": "node backend/seeds/index.js",
    "dev:frontend": "vite",
    "build:frontend": "vite build",
    "preview:frontend": "vite preview",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "multer": "^2.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "vite": "^5.4.11"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "bcrypt",
    "usuarios",
    "autenticacion"
